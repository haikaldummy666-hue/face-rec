{
  "name": "sentiweb-game-testing",
  "version": "1.0.0",
  "scripts": {
    "clean": "node -e \"const fs=require('fs');const paths=['node_modules','frontend/node_modules','backend/node_modules'];paths.forEach(p=>{try{fs.rmSync(p,{recursive:true,force:true})}catch(e){}});\"",
    "install-all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "dev": "npm run install-all && concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev"
  },
  "devDependencies": {
    "concurrently": "8.2.2"
  },
  "dependencies": {
    "jspdf": "^2.5.2"
  }
}
